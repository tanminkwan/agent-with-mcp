# Example .env file for environment variable configuration

# LLM settings
LLM_PROVIDER=ollama
LLM_MODEL=gemma3n:e4b

# OpenAI API key
OPENAI_API_KEY=your-openai-api-key-here

# vLLM 서버 URL 설정
VLLM_SERVER_URL=https://your-vllm-server-url.com

# Add more environment variables as needed

# LangSmith tracing
LANGSMITH_TRACING=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=langchain_study

# Graph selection
AGENT_GRAPH=purchase

# Optional: Node-specific LLM overrides
